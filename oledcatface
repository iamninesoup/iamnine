// =======================
// 이미지 배열
// =======================
const unsigned char* myImages[12] = {
  epd_bitmap_1,
  epd_bitmap_2,
  epd_bitmap_3,
  epd_bitmap_4,
  epd_bitmap_5,
  epd_bitmap_6,
  epd_bitmap_7,
  epd_bitmap_8,
  epd_bitmap_9,
  epd_bitmap_10,
  epd_bitmap_11,
  epd_bitmap_12
};

const int numImages = 12; // 이미지 개수

// =======================
// setup & loop
// =======================
void setup() {
  Wire.begin();
  u8g2.begin();
}

void loop() {
  for (int i = 0; i < numImages; i++) {
    u8g2.clearBuffer();
    u8g2.drawXBMP(0, 0, 128, 64, myImages[i]);
    u8g2.sendBuffer();
    delay(200); // 0.2초
  }
}
